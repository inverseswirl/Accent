{
    "env" : {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": 0,
        "semi": 0,
        "react/jsx-uses-react": "error",
       "react/jsx-uses-vars": "error",
       "react/prop-types": 0
       
    },
    "settings":{
        "react":{
            "version": "detect"
        }
    }
    
   
      
}
